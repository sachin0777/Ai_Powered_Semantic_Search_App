{
  "name": "contentstack-semantic-search-app",
  "version": "1.0.0",
  "description": "AI-Powered Semantic Search App for Contentstack",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "cd server && npm install",
    "start": "node server/server.js",
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd server && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../server && npm install",
    "sync": "cd server && npm run sync",
    "deploy": "npm run build && contentstack launch:deploy"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "keywords": [
    "contentstack",
    "semantic-search",
    "marketplace",
    "embeddings",
    "ai",
    "search"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
